
<launch4jConfig>

	<headerType>0</headerType>
	<outfile>build/Install.exe</outfile>
	<jar>build/Install.jar</jar>
	<dontWrapJar>false</dontWrapJar>
	<errTitle>@TOKEN.APP.NAME@ Install Error</errTitle>
	<jarArgs></jarArgs>
	<chdir>.</chdir>
	<customProcName>true</customProcName>
	<stayAlive>true</stayAlive>
	<icon>install.ico</icon>
	<jre>
		<minVersion>1.5.0</minVersion>
		<initialHeapSize>128</initialHeapSize>
		<maxHeapSize>256</maxHeapSize>
	</jre>
	<splash>
		<file>splash.bmp/</file>
		<waitForWindow>true</waitForWindow>
		<timeout>60</timeout>
		<timeoutErr>true</timeoutErr>
	</splash>
	<versionInfo>
		<fileVersion>2.2.5.0</fileVersion>
		<txtFileVersion>2.2.5</txtFileVersion>
		<fileDescription>@TOKEN.APP.NAME@ Windows installer</fileDescription>
		<copyright>@TOKEN.APP.NAME@, @TOKEN.COMPANY.NAME@ copyright @TOKEN.COPYRIGHT.YEAR@</copyright>
		<productVersion>@TOKEN.APP.VERSION@</productVersion>
		<txtProductVersion>@TOKEN.APP.VERSION@</txtProductVersion>
		<productName>@TOKEN.APP.NAME@  Windows Installer</productName>
		<companyName>@TOKEN.COMPANY.NAME@</companyName>
		<internalName>@TOKEN.APP.NAME.LOWCASE@.jar</internalName>
		<originalFilename>Install.exe</originalFilename>
	</versionInfo>
</launch4jConfig>